<section id="capabilities" class="">
  <!-- carousel-fade -->
  <div id="carouselCapabilities" class="carousel slide carousel-fade" data-ride="carousel">
  <!-- <div id="carouselCapabilities" class="carousel slide" data-ride="carousel"> -->
    <ol class="carousel-indicators">
      {{#capabilities3}}
      <li data-target="#carouselCapabilities" data-slide-to="{{id}}"></li>
      {{/capabilities3}}
    </ol>
    <div class="carousel-inner">

      {{#capabilities3}}
      <div class="carousel-item">
        <div class="view">
          <img class="d-block" src="assets/img/backgrounds/{{image}}" alt="{{title}}">
          <div class="mask rgba-stylish-strong"></div>
        </div>
        <div class="carousel-caption d-flex flex-column justify-content-center">
          <h1 class="h1-responsive my-4">{{title}}</h1>
          <ul class="list-unstyled">

            {{#list}}
              <li class="capabilitie-item">{{description}}</li>
            {{/list}}

          </ul>
        </div>
      </div>
      {{/capabilities3}}
    </div>
    <a class="carousel-control-prev" href="#carouselCapabilities" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselCapabilities" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</section>
