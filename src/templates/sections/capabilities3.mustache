<div id="capabilities" class="">
  <!-- carousel-fade -->
  <div id="carouselCapabilities" class="carousel slide carousel-fade">
  <!-- <div id="carouselCapabilities" class="carousel slide" data-ride="carousel"> -->
    <ol class="carousel-indicators">
      {{#capabilities3}}
      <li data-target="#carouselCapabilities" data-slide-to="{{id}}"></li>
      {{/capabilities3}}
    </ol>
    <div class="carousel-inner">

      {{#capabilities3}}
      <div class="carousel-item white-text">
        <div class="view py-5 item-image-wrapper" style="background-image: url('assets/img/backgrounds/{{image}}')">
          <div class="mask rgba-stylish-strong"></div>
          <div class="container">
            <div class="row">
              <div class="col">
                <h3 class="h3 mb-4">{{title}}</h3>
                <ul class="list-unstyled">
                  {{#list}}
                    <li class="capabilitie-item">{{description}}</li>
                  {{/list}}
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      {{/capabilities3}}
    </div>
    <a class="carousel-control-prev d-none d-md-flex" href="#carouselCapabilities" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next d-none d-md-flex" href="#carouselCapabilities" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
