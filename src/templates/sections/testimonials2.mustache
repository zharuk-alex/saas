<!--Carousel Wrapper-->
<div id="carouselTestimonials2"
  class="carousel slide carousel-fade parallax-window"
  data-ride="carousel"
  data-parallax="scroll"
  data-image-src="../assets/img/backgrounds/testimonials.png">
  <!--Indicators-->
  <ol class="carousel-indicators">
    {{#testimonials}}
    <li data-target="#carouselTestimonials2" data-slide-to="{{id}}" class="{{active}}"></li>
    {{/testimonials}}
  </ol>
  <!--/.Indicators-->
  <!--Slides-->
  <div class="carousel-inner" role="listbox">
    <!--First slide-->
    {{#testimonials}}
    <div class="carousel-item {{active}} testimonial-item">
      <img src="../assets/img/testimonials/{{user_image}}" alt="">
      <blockquote>
        <p>{{testimonial}}</p>
      </blockquote>
      <hr class="header-divider my-2">
      <h5>{{position}}</h5>
    </div>
    {{/testimonials}}
    <!--/First slide-->
  </div>
  <!--/.Slides-->
  <!--Controls-->
  <a class="carousel-control-prev" href="#carouselTestimonials2" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselTestimonials2" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
  <!--/.Controls-->
</div>
<!--/.Carousel Wrapper-->
