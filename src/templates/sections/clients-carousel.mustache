<section id="clients" class="pt-5 wow fadeIn clients">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        {{#clients}}
        <h2 class="h2 mb-2 text-center">{{title}}</h2>
        <hr class="header-divider mb-5">
        <div id="clients-carousel" class="owl-carousel owl-theme mt-2 mb-5 wow fadeInRight">
          {{#client_item}}
          <div class="item">
            <img src="../assets/img/clients/{{ id }}-logo.jpg" alt="{{ name }}" class="img-fluid">
          </div>
          {{/client_item}}
        </div>
        {{/clients}}
      </div>
    </div>
  </div>
</section>
